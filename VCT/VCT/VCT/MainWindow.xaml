            <Label x:Name="lbText1" Margin="-470, 0,190,0" Content="Text Test" FontFamily="Times New Roman" Foreground="White" FontSize="20" HorizontalContentAlignment="Center"/>
            <Label x:Name="lbTag1" Margin="-190, 0,75,0" Content="Tag Test" FontFamily="Times New Roman" Foreground="White" FontSize="20" HorizontalContentAlignment="Center"/>
            <Button x:Name="btnTrain1" Margin="-70,5,5,5" Content="Train" FontFamily="Times New Roman" Foreground="White" Background="#FF2F8160" BorderThickness="2" BorderBrush="#FF99E450" FontSize="20"/>
            <Button x:Name="btnDelete1" Margin="0,9,0,9" Width="55" Content="Delete" FontFamily="Times New Roman" Foreground="White" Background="#FFC32A2A" BorderThickness="2" BorderBrush="#FF610303" FontSize="15" VerticalContentAlignment="Center"/>
